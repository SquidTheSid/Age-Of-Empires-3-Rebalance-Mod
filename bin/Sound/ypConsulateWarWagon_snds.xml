<?xml version='1.0' encoding='utf-8'?>
<protounitsounddef>
  <protounit name="ypConsulateWarWagon">
    <soundtype name="Select">
      <soundset name="GermanWarWagonSelect"/>
    </soundtype>
    <soundtype name="Grunt">
      <soundset name="CavalryGrunt"/>
    </soundtype>
    <soundtype name="Death">
      <soundset name="GenericMaleDeath"/>
    </soundtype>
    <soundtype name="Creation">
      <soundset name="MilitaryBirth"/>
    </soundtype>
    <soundtype name="Acknowledge">
      <targetlogic>
        <choice name="default">
          <soundset name="GermanWarWagonAcknowledge"/>
        </choice>
        <choice name="enemy">
          <soundset name="GermanWarWagonAttack"/>
        </choice>
      </targetlogic>
    </soundtype>
  </protounit>
</protounitsounddef>
