<?xml version='1.0' encoding='utf-8'?>
<protounitsounddef>
  <protounit name="Ranger">
    <soundtype name="Select">
      <soundset name="BillyHolmeSelect"/>
    </soundtype>
    <soundtype name="Grunt">
      <soundset name="BillyHolmeGrunt"/>
    </soundtype>
    <soundtype name="Death">
      <soundset name="BillyHolmeDeath"/>
    </soundtype>
    <soundtype name="Creation">
      <soundset name="SettlerBirth"/>
    </soundtype>
    <soundtype name="Acknowledge">
      <targetlogic>
        <choice name="default">
          <soundset name="BillyHolmeAcknowledge"/>
        </choice>
        <choice name="enemy">
          <soundset name="BillyHolmeAttack"/>
        </choice>
        <choice name="build"/>
        <choice name="Huntable"/>
        <choice name="Tree"/>
        <choice name="AbstractMine"/>
        <choice name="AbstractFarm"/>
        <choice name="AbstractResourceCrate"/>
        <choice name="AbstractFruit"/>
      </targetlogic>
    </soundtype>
    <soundtype name="KnockOutRevived">
      <soundset name="BillyHolmeRevive"/>
    </soundtype>
  </protounit>
</protounitsounddef>
