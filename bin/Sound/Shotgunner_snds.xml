<?xml version='1.0' encoding='utf-8'?>
<protounitsounddef>
  <protounit name="Shotgunner">
    <soundtype name="Select">
      <soundset name="AmeliaBlackSelect"/>
    </soundtype>
    <soundtype name="Grunt">
      <soundset name="AmeliaBlackGrunt"/>
    </soundtype>
    <soundtype name="Death">
      <soundset name="AmeliaBlackDeath"/>
    </soundtype>
    <soundtype name="Creation">
      <soundset name="SettlerBirth"/>
    </soundtype>
    <soundtype name="Acknowledge">
      <targetlogic>
        <choice name="default">
          <soundset name="AmeliaBlackAcknowledge"/>
        </choice>
        <choice name="enemy">
          <soundset name="AmeliaBlackAttack"/>
        </choice>
        <choice name="build"/>
        <choice name="Huntable"/>
        <choice name="Tree"/>
        <choice name="AbstractMine"/>
        <choice name="AbstractFarm"/>
        <choice name="AbstractResourceCrate"/>
        <choice name="AbstractFruit"/>
      </targetlogic>
    </soundtype>
    <soundtype name="KnockOutRevived">
      <soundset name="AmeliaBlackRevive"/>
    </soundtype>
  </protounit>
</protounitsounddef>
